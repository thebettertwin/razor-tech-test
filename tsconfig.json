{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@context": ["src/context"],
      "@context/*": ["src/context/*"],
      "@api": ["src/api"],
      "@api/*": ["src/api/*"],
      "@config": ["src/config"],
      "@config/*": ["src/config/*"],
      "@helpers": ["src/helpers"],
      "@helpers/*": ["src/helpers/*"]
    }
  }
}
